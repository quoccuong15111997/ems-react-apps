{
  "name": "@progress/kendo-react-layout",
  "version": "5.12.1",
  "description": "React Layout components enable you to create a perceptive and intuitive layout of web projects. KendoReact Layout package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "scripts": {
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/layout/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/layout/e2e/",
    "e2e-axe": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/layout/e2e/accessibility/a11y.tests.ts",
    "start": "gulp start",
    "build-package": "gulp build-package",
    "ssr": "gulp bundle-ssr-server bundle-ssr-client && node dist/ssr/server.js"
  },
  "homepage": "https://www.telerik.com/kendo-react-ui",
  "keywords": [
    "Kendo UI",
    "React",
    "Progress",
    "TabStrip",
    "PanelBar",
    "Splitter",
    "Menu",
    "Drawer",
    "KendoReact",
    "reactjs",
    "UI",
    "components",
    "React component",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-react-intl": "^5.0.0",
    "@progress/kendo-react-progressbars": "^5.0.0",
    "@progress/kendo-svg-icons": "^1.0.0",
    "react": "^16.8.2 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.2 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@progress/kendo-data-query": "^1.5.0",
    "@progress/kendo-date-math": "^1.5.1",
    "@progress/kendo-drawing": "^1.17.2",
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-react-buttons": "5.12.1",
    "@progress/kendo-react-charts": "5.12.1",
    "@progress/kendo-react-dateinputs": "5.12.1",
    "@progress/kendo-react-dropdowns": "5.12.1",
    "@progress/kendo-react-grid": "5.12.1",
    "@progress/kendo-react-indicators": "5.12.1",
    "@progress/kendo-react-inputs": "5.12.1",
    "@progress/kendo-react-intl": "5.12.1",
    "@progress/kendo-react-progressbars": "5.12.1",
    "@progress/kendo-svg-icons": "^1.0.0"
  },
  "dependencies": {
    "@progress/kendo-react-animation": "5.12.1",
    "@progress/kendo-react-common": "5.12.1",
    "@progress/kendo-react-popup": "5.12.1",
    "prop-types": "^15.6.0"
  },
