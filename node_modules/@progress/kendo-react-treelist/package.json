{
  "name": "@progress/kendo-react-treelist",
  "version": "5.12.1",
  "description": "React TreeList enables the display of self-referencing tabular data. KendoReact TreeList package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "scripts": {
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/treelist/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/treelist/e2e/",
    "e2e-axe": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/treelist/e2e/accessibility.tests.ts",
    "start": "gulp start",
    "build-package": "gulp build-package",
    "ssr": "gulp bundle-ssr-server bundle-ssr-client && node dist/ssr/server.js"
  },
  "keywords": [
    "Kendo UI",
    "React",
    "Progress",
    "TreeList",
    "KendoReact",
    "reactjs",
    "UI",
    "components",
    "React component",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-data-query": "^1.0.0",
    "@progress/kendo-date-math": "^1.4.1",
    "@progress/kendo-drawing": "^1.17.2",
