    "@progress/kendo-react-dropdowns": "5.12.1",
    "@progress/kendo-react-excel-export": "5.12.1",
    "@progress/kendo-react-inputs": "5.12.1",
    "@progress/kendo-react-intl": "5.12.1",
    "@progress/kendo-react-pdf": "5.12.1",
    "@progress/kendo-svg-icons": "^1.0.0"
  },
